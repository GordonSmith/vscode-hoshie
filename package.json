{
  "private": true,
  "name": "hoshie",
  "displayName": "Hoshie Language",
  "description": "Functional Data Processing Language",
  "publisher": "",
  "version": "0.0.1",
  "categories": [
    "Programming Languages"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "lint": "eslint src --ext ts",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@hpcc-js/util": "^2.11.0",
    "@types/glob": "^7.1.1",
    "@types/mocha": "^7.0.1",
    "@types/node": "^12.11.7",
    "@types/vscode": "^1.43.0",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "chevrotain": "^6.5.0",
    "eslint": "^6.8.0",
    "glob": "^7.1.6",
    "mocha": "^7.0.1",
    "typescript": "^3.7.5",
    "vscode-test": "^1.3.0"
  },
  "engines": {
    "vscode": "^1.43.0"
  },
  "activationEvents": [
    "onLanguage:hoshie"
  ],
  "contributes": {
    "languages": [
      {
        "id": "hoshie",
        "aliases": [
          "Hoshie",
          "hoshie"
        ],
        "extensions": [
          ".ho"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "hoshie",
        "scopeName": "source.hoshie",
        "path": "./syntaxes/hoshie.tmLanguage.json"
      }
    ],
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ]
  }
}
